nmap <space> <c-f>
nmap q :q<enter>
nmap Q :qa!<enter>
nmap <c-n> :set nu!<enter>
nmap <c-p> :set rnu!<enter>
nmap / /\v
set shiftwidth=4
set softtabstop=4
set expandtab
set ruler
set laststatus=2
set nostartofline
set backspace=indent,eol,start
set ignorecase
set smartcase
set hlsearch
filetype plugin indent on
filetype indent on
